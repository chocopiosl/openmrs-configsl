    "config": {
      "widget": "obsgraph",
      "icon": "fas fa-fw fa-chart-bar",
      "label": "pih.app.hivWeightGraph.title",
      "conceptId": "${concept.weightKG.uuid}",
      "duringCurrentEnrollmentInProgram": "${program.infant.uuid}",
      "maxResults": "100"
    }
  },
  {
    "id": "infant.dashboard.currentEnrollment",
    "extensions": [
      {
